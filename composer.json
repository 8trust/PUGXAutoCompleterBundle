{
    "name": "pugx/autocompleter-bundle",
    "type": "symfony-bundle",
    "description": "Add an autocomplete type to forms",
    "keywords": ["Symfony", "form", "field"],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "garakkio@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "doctrine/orm": "^2.5",
        "symfony/framework-bundle": "^2.8|^3.2",
        "symfony/form": "^2.8|^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0",
        "lexik/form-filter-bundle": "^5.0"
    },
    "suggest": {
        "lexik/form-filter-bundle" : "To use autocomplete on a filter."
    },
    "autoload": {
        "psr-4": { "PUGX\\AutocompleterBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5.x-dev"
        }
    }
}
